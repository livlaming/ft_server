server {
	listen 80;
	listen [::]:80;

	server_name localhost;
	root var/www/html;
	# return 301 https://localhost$request_uri;

    index index.php index.html index.htm index.nginx-debian.html;

    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/run/php/php7.3-fpm.sock;
    }
}
